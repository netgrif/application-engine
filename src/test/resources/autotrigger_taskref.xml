<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
	<id>Praca</id>
	<version>1.0.0</version>
	<title>Práca</title>
	<defaultRole>true</defaultRole>
	<anonymousRole>false</anonymousRole>
	<properties>
		<property key="initials">PRC</property>
	</properties>
	<data type="taskRef">
		<id>tema</id>
		<title/>
		<init>t2</init>
	</data>
	<transition>
		<id>t1</id>
		<x>300</x>
		<y>60</y>
		<title>Téma</title>
		<flex>
			<id>t1_flex</id>
		</flex>
	</transition>
	<transition>
		<id>t2</id>
		<x>100</x>
		<y>140</y>
		<title>Init</title>
		<trigger type="auto"/>
		<flex>
			<id>t2_flex</id>
		</flex>
		<event type="finish">
			<id>t2_finish</id>
			<actions phase="pre">
				<action id="action_0">
					<![CDATA[
					change tema value {
						[useCase.getTaskStringId("t1")]
					}
					]]>
				</action>
			</actions>
		</event>
	</transition>
	<place>
		<id>p1</id>
		<x>20</x>
		<y>140</y>
		<tokens>1</tokens>
	</place>
	<place>
		<id>p2</id>
		<x>180</x>
		<y>140</y>
		<tokens>0</tokens>
	</place>
	<arc>
		<id>a1</id>
		<type>regular</type>
		<sourceId>p1</sourceId>
		<destinationId>t2</destinationId>
		<multiplicity>1</multiplicity>
	</arc>
	<arc>
		<id>a2</id>
		<type>regular</type>
		<sourceId>t2</sourceId>
		<destinationId>p2</destinationId>
		<multiplicity>1</multiplicity>
	</arc>
</process>