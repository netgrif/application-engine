<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
    <id>remote_file_field_net</id>
    <version>1.0.0</version>
    <title>Test</title>
    <defaultRole>true</defaultRole>
    <data type="fileList">
        <id>fileList</id>
        <title>Remote File List</title>
        <placeholder>Remote file list</placeholder>
        <init dynamic="true">FileListFieldValue.fromString("test-file.txt:https://netgrif.com,test-file-list.txt:https://netgrif.com")</init>
        <properties>
            <property key="remote">true</property>
        </properties>
    </data>
    <transition>
        <id>task</id>
        <x>0</x>
        <y>0</y>
        <title>Task</title>
        <flex>
            <id>task_flex</id>
            <item>
                <dataRef>
                    <id>fileList</id>
                    <logic>
                        <behavior>visible</behavior>
                    </logic>
                </dataRef>
            </item>
        </flex>
    </transition>
</process>