    <data type="file">
        <id>file</id>
        <title>File</title>
        <action trigger="set">
            <!-- @formatter:off -->
            textfield: f.text;
            change textfield value {
            "funguje to"
            }
            <!-- @formatter:on -->
        </action>
    </data>

    <data type="fileList">
        <id>fileList</id>
        <title>File List</title>
    </data>

    <data type="user">
        <id>user</id>
        <title>User</title>
    </data>

    <data type="dateTime">
        <id>datetime</id>
        <title>Datetime</title>
    </data>

    <data type="button">
        <id>button</id>
        <title>Button</title>
        <placeholder>Push</placeholder>
        <component>
            <name>fab</name>
        </component>
    </data>
