<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
    <id>test</id>
    <version>1.0.0</version>
    <title>Test</title>
    <defaultRole>false</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">TST</property>
    </properties>
    <processEvents/>
    <caseEvents/>
    <data type="number">
        <id>data_number</id>
        <title>Number</title>
    </data>
    <data type="text">
        <id>data_text</id>
        <title>Text</title>
    </data>
    <transition>
        <id>t2</id>
        <x>380</x>
        <y>180</y>
        <title>Enabled</title>
        <flex>
            <id>t2_flex</id>
            <item>
                <dataRef>
                    <id>data_text</id>
                    <logic>
                        <behavior>visible</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>data_number</id>
                    <logic>
                        <behavior>visible</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
        </flex>
        <event type="assign">
            <id>t2_assign</id>
            <actions phase="post">
                <action id="action_2">
					<![CDATA[
                    def usecase = findCase({ it.title.eq("Case 1") })
                    setData("t2", usecase, new DataSet([
                            "data_text": new TextField(rawValue: "some text"),
                            "data_number": new NumberField(rawValue: 10)
                    ]))
                    ]]>
				</action>
            </actions>
        </event>
    </transition>
    <place>
        <id>p1</id>
        <x>220</x>
        <y>180</y>
        <tokens>1</tokens>
    </place>
    <arc>
        <id>a4</id>
        <type>regular</type>
        <sourceId>p1</sourceId>
        <destinationId>t2</destinationId>
        <multiplicity>1</multiplicity>
    </arc>
</process>