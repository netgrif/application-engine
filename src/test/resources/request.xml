    <transition>
        <id>t1</id>
        <x>496</x>
        <y>208</y>
        <title>Submit request</title>
        <roleRef>
            <id>user</id>
            <logic>
                <perform>true</perform>
            </logic>
        </roleRef>
        <flex>
            <id>t1_0</id>
            <item>
                <dataRef>
                    <id>taskRef_0</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
            </item>
        </flex>
        <event type="finish">
            <id>t1_finish</id>
            <actions phase="pre">
                <action id="1">
					<![CDATA[
                    change text_6 value { "Request submitted." }
                    ]]>
				</action>
            </actions>
        </event>
    </transition>
    <transition>
        <id>t2</id>
        <x>752</x>
        <y>208</y>
        <title>Registration</title>
        <roleRef>
            <id>registration</id>
            <logic>
                <perform>true</perform>
            </logic>
        </roleRef>
        <flex>
            <id>t2_0</id>
            <item>
                <dataRef>
                    <id>enumeration_0</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                    <component>
                        <id>list</id>
                    </component>
                </dataRef>
            </item>
            <item>
                <dataRef>
                    <id>taskRef_0</id>
                    <logic>
                        <behavior>visible</behavior>
                    </logic>
                </dataRef>
            </item>
        </flex>
        <event type="finish">
            <id>t2_finish</id>
            <actions phase="pre">
                <action id="2">
					<![CDATA[
                    change text_6 value { "Request registered." }
                    ]]>
				</action>
            </actions>
            <actions phase="post">
                <action id="4">
					<![CDATA[
                    if (number_0.value < 2000) {
                        assignTask("t3")
                        finishTask("t3")
                    } else {
                        assignTask("t6")
                        finishTask("t6")
                    }
                    ]]>
				</action>
            </actions>
        </event>
    </transition>
    <transition>
        <id>t3</id>
        <x>976</x>
        <y>176</y>
        <title>To first</title>
        <roleRef>
