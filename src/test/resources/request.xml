        <event type="finish">
            <id>t5_finish</id>
            <actions phase="pre">
                <action id="3">
					<![CDATA[
                    change text_6 value { "Request finished." }
                    make text_5, visible on t8 when { true }
                    ]]>
				</action>
            </actions>
        </event>
    </transition>
