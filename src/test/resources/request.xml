        <id>enumeration_0</id>
        <title>Origin of the request</title>
        <options>
            <option key="Online">Online</option>
            <option key="Call">Call</option>
        </options>
    </data>
    <data type="number">
        <id>number_0</id>
        <title>Customer ID</title>
        <validations>
            <validation>
                <name>inrange</name>
                <arguments type="server">
                    <argument>1000</argument>
                    <argument>2999</argument>
                </arguments>
            </validation>
        </validations>
    </data>
    <data type="taskRef">
        <id>taskRef_0</id>
        <title/>
        <init>t9</init>
    </data>
    <data type="text">
        <id>text_0</id>
        <title>Name</title>
    </data>
    <data type="text">
