<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
    <id>test</id>
    <version>1.0.0</version>
    <title>Test</title>
    <defaultRole>true</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">TST</property>
    </properties>
    <processEvents/>
    <caseEvents/>
    <data type="number">
        <id>count</id>
        <title>Count</title>
        <init>0</init>
    </data>
    <data type="number">
        <id>field</id>
        <title>Field</title>
        <init>0</init>
    </data>
    <data type="number">
        <id>paged</id>
        <title>Paged</title>
        <init>0</init>
    </data>
    <transition>
        <id>synchronized</id>
        <x>0</x>
        <y>0</y>
        <title>Synchronized</title>
    </transition>
    <transition>
        <id>task</id>
        <x>0</x>
        <y>0</y>
        <title>Task</title>
        <event type="finish">
            <id>t1</id>
            <actions phase="post">
                <action id="action_1">
					<![CDATA[
                    execute "synchronized" where {
                        it.title.eq("Case 1")
                    } with (new DataSet([
                            "field": new NumberField(rawValue: 69.0)
                    ]))
                    ]]>
				</action>
                <action id="action_2">
					<![CDATA[
                    change field about {
                        findCase({it.title.eq("Case 1")});
                        return 0 as Double;
                    }
                    ]]>
				</action>
                <action id="action_3">
					<![CDATA[
                    def cases = findCases({it.title.startsWith("Case ")});
                    change count value { return cases.size(); }
                    ]]>
				</action>
                <action id="action_4">
					<![CDATA[
                    def cases = findCases({ it.title.startsWith("Case ")}, org.springframework.data.domain.PageRequest.of(1, 4));
                    change paged value { return cases.size(); }
                    ]]>
				</action>
            </actions>
        </event>
    </transition>
</process>