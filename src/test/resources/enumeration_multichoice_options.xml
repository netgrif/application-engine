<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
    <id>enumeration_multichoice_options</id>
    <version>1.0.0</version>
    <title>Enumeration/multichoice options</title>
    <defaultRole>true</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">EMO</property>
    </properties>
    <data type="enumeration" immediate="true">
        <id>enumeration</id>
        <title>Enumeration values</title>
        <options>
            <option key="Alice">Alice</option>
            <option key="Bob">Bob</option>
            <option key="Carol">Carol</option>
        </options>
        <init>Bob</init>
    </data>
    <data type="enumeration" immediate="true">
        <id>enumeration_like_map</id>
        <title>Enumeration options</title>
        <options>
            <option key="one">Alice</option>
            <option key="two">Bob</option>
            <option key="three">Carol</option>
        </options>
        <init>Bob</init>
    </data>
    <data type="multichoice" immediate="true">
        <id>multichoice</id>
        <title>Multichoice values</title>
        <options>
            <option key="Alice">Alice</option>
            <option key="Bob">Bob</option>
            <option key="Carol">Carol</option>
        </options>
        <init>Alice,Bob</init>
    </data>
    <data type="multichoice" immediate="true">
        <id>multichoice_like_map</id>
        <title>Multichoice options</title>
        <options>
            <option key="one">Alice</option>
            <option key="two">Bob</option>
            <option key="three">Carol</option>
        </options>
        <init>Alice,Bob</init>
    </data>
    <transition>
        <id>t1</id>
        <x>379</x>
        <y>273</y>
        <title>Task - editable</title>
        <assignPolicy>auto</assignPolicy>
        <flex>
            <id>t1_flex</id>
            <item>
                <dataRef>
                    <id>enumeration</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>enumeration_like_map</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>multichoice</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>multichoice_like_map</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
        </flex>
    </transition>
</process>