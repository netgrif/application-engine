<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
    <id>change_value</id>
    <version>1.0.0</version>
    <title>Test</title>
    <defaultRole>true</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">TST</property>
    </properties>
    <processEvents/>
    <caseEvents/>
    <data type="boolean">
        <id>addExisting</id>
        <title>Switch</title>
        <event type="set">
            <id>addExisting_set</id>
            <actions phase="post">
                <action id="action_2">
					<![CDATA[
                    change caseref value {return [useCase.stringId]}
                    ]]>
				</action>
            </actions>
        </event>
    </data>
    <data type="boolean">
        <id>addInvalidNet</id>
        <title>Switch</title>
        <event type="set">
            <id>addInvalidNet_set</id>
            <actions phase="post">
                <action id="action_4">
					<![CDATA[
                    def newCase = createCase("test");
                    try {
                        change caseref value {return caseref.rawValue + [newCase.stringId]}
                    } catch (IllegalArgumentException e) {}
                    ]]>
				</action>
            </actions>
        </event>
    </data>
    <data type="boolean">
        <id>addNew</id>
        <title>Switch</title>
        <event type="set">
            <id>addNew_set</id>
            <actions phase="post">
                <action id="action_3">
					<![CDATA[
                    def newCase = createCase("change_value")
                    change caseref value {return caseref.rawValue + [newCase.stringId]}
                    ]]>
				</action>
            </actions>
        </event>
    </data>
    <data type="caseRef">
        <id>caseref</id>
        <title>Caseref</title>
        <allowedNets>
            <allowedNet>change_value</allowedNet>
        </allowedNets>
    </data>
    <transition>
        <id>t1</id>
        <x>1</x>
        <y>1</y>
        <title>Tran</title>
        <flex>
            <id>t1_flex</id>
            <item>
                <dataRef>
                    <id>addExisting</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>addNew</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>addNew</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
        </flex>
    </transition>
</process>