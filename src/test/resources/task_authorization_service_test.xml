<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
    <id>wst</id>
    <version>1.0.0</version>
    <title>WorkflowAuthorizationService test</title>
    <defaultRole>false</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">WST</property>
    </properties>
    <processEvents/>
    <caseEvents/>
    <role>
        <id>assign_neg_role</id>
        <title>assign neg role</title>
    </role>
    <role>
        <id>assign_pos_role</id>
        <title>assign pos role</title>
    </role>
    <role>
        <id>finish_neg_role</id>
        <title>finish neg role</title>
    </role>
    <role>
        <id>finish_pos_role</id>
        <title>finish pos role</title>
    </role>
    <data type="file">
        <id>file</id>
        <title>File</title>
    </data>
    <data type="text">
        <id>text</id>
        <title>Text</title>
        <init>text</init>
    </data>
    <transition>
        <id>t1</id>
        <x>1</x>
        <y>1</y>
        <title>Transition</title>
        <roleRef>
            <id>assign_neg_role</id>
            <permission>
                <assign>false</assign>
            </permission>
        </roleRef>
        <roleRef>
            <id>assign_pos_role</id>
            <permission>
                <assign>true</assign>
            </permission>
        </roleRef>
        <flex>
            <id>t1_flex</id>
            <item>
                <dataRef>
                    <id>text</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>file</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
        </flex>
    </transition>
</process>