        <id>text1</id>
        <title>Text1</title>
        <event type="set">
            <id>text1_set</id>
            <actions phase="post">
                <action id="action_0">
					<![CDATA[
                    change text2 value { text1.value }
                    ]]>
				</action>
            </actions>
        </event>
    </data>
