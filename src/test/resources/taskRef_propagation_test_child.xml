					<![CDATA[
                    def parentCase = workflowService.findOne(parentId.value)
                    setDataWithPropagation("t2", parentCase, [
                            "text_from_child_setter": ["type": "text", "value": (text2.value + "-propagated-up") as String]
                    ])
                    ]]>
				</action>
            </actions>
        </event>
    </data>
