<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
	<id>case_name_change_test</id>
	<version>1.0.0</version>
	<title>Case name change Test</title>
	<icon>device_hub</icon>
	<defaultRole>true</defaultRole>
	<anonymousRole>true</anonymousRole>
	<properties>
		<property key="initials">CNC</property>
	</properties>
	<processEvents/>
	<caseEvents/>
	<data type="boolean">
		<id>bln</id>
		<title/>
		<event type="set">
			<id>bln_set</id>
			<actions phase="post">
				<action id="action_0">
					<![CDATA[
					changeCaseProperty "title" about { "Set action title" }
					]]>
				</action>
			</actions>
		</event>
	</data>
	<transition>
		<id>t1</id>
		<x>580</x>
		<y>100</y>
		<title>Transition 1</title>
		<flex>
			<id>t1_flex</id>
			<item>
				<dataRef>
					<id>bln</id>
					<logic>
						<behavior>editable</behavior>
					</logic>
				</dataRef>
				<properties>
					<order>0</order>
					<flex-grow>0</flex-grow>
					<flex-shrink>1</flex-shrink>
				</properties>
			</item>
		</flex>
		<event type="assign">
			<id>test_event0</id>
			<actions phase="pre">
				<action id="action_1">
					<![CDATA[
					changeCaseProperty "title" about { "Pre assign title" }
					]]>
				</action>
			</actions>
		</event>
		<event type="cancel">
			<id>test_event2</id>
			<actions phase="pre">
				<action id="action_2">
					<![CDATA[
					changeCaseProperty "title" about { "Pre cancel title" }
					]]>
				</action>
			</actions>
		</event>
		<event type="reassign">
			<id>test_event4</id>
			<actions phase="pre">
				<action id="action_3">
					<![CDATA[
					changeCaseProperty "title" about { "Pre delegate title" }
					]]>
				</action>
			</actions>
		</event>
	</transition>
	<transition>
		<id>t2</id>
		<x>580</x>
		<y>220</y>
		<title>Transition 2</title>
	</transition>
	<transition>
		<id>t3</id>
		<x>900</x>
		<y>100</y>
		<title/>
	</transition>
	<place>
		<id>p1</id>
		<x>420</x>
		<y>100</y>
		<title>init</title>
		<tokens>1</tokens>
	</place>
	<place>
		<id>p2</id>
		<x>740</x>
		<y>100</y>
		<tokens>0</tokens>
	</place>
	<arc>
		<id>a1</id>
		<type>regular</type>
		<sourceId>p1</sourceId>
		<destinationId>t1</destinationId>
		<multiplicity>1</multiplicity>
		<breakpoint>
			<x>500</x>
			<y>60</y>
		</breakpoint>
	</arc>
	<arc>
		<id>a2</id>
		<type>regular</type>
		<sourceId>t1</sourceId>
		<destinationId>p1</destinationId>
		<multiplicity>1</multiplicity>
		<breakpoint>
			<x>500</x>
			<y>140</y>
		</breakpoint>
	</arc>
	<arc>
		<id>a3</id>
		<type>regular</type>
		<sourceId>t1</sourceId>
		<destinationId>p2</destinationId>
		<multiplicity>1</multiplicity>
	</arc>
	<arc>
		<id>a4</id>
		<type>regular</type>
		<sourceId>p2</sourceId>
		<destinationId>t3</destinationId>
		<multiplicity>1</multiplicity>
	</arc>
</process>