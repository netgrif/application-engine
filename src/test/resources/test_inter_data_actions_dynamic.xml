<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
    <id>test</id>
    <version>1.0.0</version>
    <title>Test</title>
    <defaultRole>true</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">TST</property>
    </properties>
    <processEvents/>
    <caseEvents/>
    <data type="number">
        <id>data1</id>
        <title>Cost</title>
        <event type="set">
            <id>data1_set</id>
            <actions phase="post">
                <action id="action_0">
					<![CDATA[
                    execute "static_edit_limits" with ([
                            "delta_limit": [
                                    value: data1.value,
                                    type: "number",
                            ]
                    ] as Map)
                    ]]>
				</action>
            </actions>
        </event>
    </data>
    <data type="number">
        <id>data2</id>
        <title>Available</title>
    </data>
    <transition>
        <id>t2</id>
        <x>270</x>
        <y>108</y>
        <title>T2</title>
        <flex>
            <id>t2_flex</id>
            <item>
                <dataRef>
                    <id>data1</id>
                    <logic>
                        <behavior>editable</behavior>
                        <required>true</required>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
        </flex>
    </transition>
    <transition>
        <id>dynamic_edit_available</id>
        <x>100</x>
        <y>100</y>
        <title>Edit available</title>
        <flex>
            <id>dynamic_edit_available_flex</id>
            <item>
                <dataRef>
                    <id>data2</id>
                    <logic>
                        <behavior>editable</behavior>
                        <required>true</required>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
        </flex>
    </transition>
    <place>
        <id>p1</id>
        <x>134</x>
        <y>111</y>
        <title>IN</title>
        <tokens>1</tokens>
    </place>
    <arc>
        <id>a3</id>
        <type>regular</type>
        <sourceId>p1</sourceId>
        <destinationId>t2</destinationId>
        <multiplicity>1</multiplicity>
    </arc>
</process>