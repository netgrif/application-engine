<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
    <id>testing_model</id>
    <version>1.0.0</version>
    <title>Testing Model</title>
    <defaultRole>false</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">TSM</property>
    </properties>
    <userRef>
        <id>user_list_1</id>
        <permission>
            <view>true</view>
        </permission>
    </userRef>
    <processEvents/>
    <caseEvents/>
    <role>
        <id>admin</id>
        <title>Admin Role</title>
    </role>
    <role>
        <id>enabledTester</id>
        <title>Enabled Testing Role</title>
    </role>
    <role>
        <id>tester</id>
        <title>Testing Role</title>
    </role>
    <data type="button">
        <id>assign</id>
        <title>Assign to user ref</title>
    </data>
    <data type="text">
        <id>email</id>
        <title>Email</title>
    </data>
    <data type="button">
        <id>remove</id>
        <title>Remove from user ref</title>
    </data>
    <data type="userList">
        <id>user_list_1</id>
        <title>User list 1</title>
    </data>
    <data type="userList">
        <id>user_list_2</id>
        <title>User list 2</title>
    </data>
    <transition>
        <id>t1</id>
        <x>380</x>
        <y>260</y>
        <title>Initialization</title>
        <flex>
            <id>t1_flex</id>
            <item>
                <dataRef>
                    <id>email</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>assign</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                    <event type="set">
                        <id>assign</id>
                        <actions phase="pre">
                            <action id="action_0">
								<![CDATA[
                                change user_list_1 value { [userService.findByEmail(email.value, true).id] }
                                ]]>
							</action>
                        </actions>
                    </event>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>remove</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                    <event type="set">
                        <id>remove</id>
                        <actions phase="pre">
                            <action id="action_1">
								<![CDATA[
                                change user_list_1 value { [] }
                                ]]>
							</action>
                        </actions>
                    </event>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
        </flex>
        <roleRef>
            <id>enabledTester</id>
            <permission>
                <view>true</view>
                <perform>true</perform>
            </permission>
        </roleRef>
        <roleRef>
            <id>user_list_1</id>
            <permission>
                <view>true</view>
            </permission>
        </roleRef>
    </transition>
    <transition>
        <id>t2</id>
        <x>620</x>
        <y>260</y>
        <title>Finishing</title>
    </transition>
    <place>
        <id>p1</id>
        <x>260</x>
        <y>260</y>
        <title>#init</title>
        <tokens>1</tokens>
    </place>
    <place>
        <id>p2</id>
        <x>500</x>
        <y>260</y>
        <title>After initialization state</title>
        <tokens>0</tokens>
    </place>
    <arc>
        <id>a1</id>
        <type>regular</type>
        <sourceId>p1</sourceId>
        <destinationId>t1</destinationId>
        <multiplicity>1</multiplicity>
    </arc>
    <arc>
        <id>a2</id>
        <type>regular</type>
        <sourceId>t1</sourceId>
        <destinationId>p2</destinationId>
        <multiplicity>1</multiplicity>
    </arc>
    <arc>
        <id>a3</id>
        <type>regular</type>
        <sourceId>p2</sourceId>
        <destinationId>t2</destinationId>
        <multiplicity>1</multiplicity>
    </arc>
    <arc>
        <id>a4</id>
        <type>regular</type>
        <sourceId>t2</sourceId>
        <destinationId>p1</destinationId>
        <multiplicity>1</multiplicity>
        <breakpoint>
            <x>460</x>
            <y>140</y>
        </breakpoint>
    </arc>
</process>