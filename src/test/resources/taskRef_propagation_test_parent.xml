<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
    <id>parent</id>
    <version>1.0.0</version>
    <title>parent</title>
    <icon>home</icon>
    <defaultRole>true</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">PAR</property>
    </properties>
    <processEvents/>
    <caseEvents/>
    <data type="taskRef">
        <id>children_tasks</id>
        <title>TaskRef</title>
    </data>
    <data type="file">
        <id>file</id>
        <title>File</title>
    </data>
    <data type="multichoice">
        <id>multichoice</id>
        <title>Multichoice</title>
    </data>
    <data type="text">
        <id>multichoice_setter</id>
        <title>Multichoice Setter</title>
        <event type="set">
            <id>multichoice_setter_set</id>
            <actions phase="post">
                <action id="action_7">
					<![CDATA[
                    def newChoices = multichoice_setter.value.split(";") as List
                    change multichoice choices { newChoices }
                    ]]>
				</action>
            </actions>
        </event>
    </data>
    <data type="number">
        <id>number</id>
        <title>Number</title>
    </data>
    <data type="text">
        <id>text</id>
        <title>Text</title>
        <event type="set">
            <id>text_set</id>
            <actions phase="pre">
                <action id="action_5">
					<![CDATA[
                    children_tasks.value.each {
                        setDataWithPropagation(it as String, [
                                "text3": ["type": "text", "value": ((text.value ?: "") + "-propagated-down-pre") as String]
                        ])
                    }
                    ]]>
				</action>
            </actions>
            <actions phase="post">
                <action id="action_6">
					<![CDATA[
                    children_tasks.value.each {
                        setDataWithPropagation(it as String, [
                                "text1": ["type": "text", "value": (text.value + "-propagated-down-post") as String]
                        ])
                    }
                    ]]>
				</action>
            </actions>
        </event>
    </data>
    <data type="text">
        <id>text_from_child</id>
        <title>Text from child</title>
    </data>
    <data type="text">
        <id>text_from_child_setter</id>
        <title>Text from child setter</title>
        <event type="set">
            <id>text_from_child_setter_set</id>
            <actions phase="post">
                <action id="action_8">
					<![CDATA[
                    change text_from_child value { text_from_child_setter.value as String }
                    ]]>
				</action>
            </actions>
        </event>
    </data>
    <transition>
        <id>t2</id>
        <x>780</x>
        <y>220</y>
        <title>Task 2</title>
        <assignPolicy>auto</assignPolicy>
        <flex>
            <id>t2_flex</id>
            <item>
                <dataRef>
                    <id>file</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>text</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>text_from_child</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>multichoice</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>multichoice_setter</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>children_tasks</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
        </flex>
    </transition>
    <transition>
        <id>t4</id>
        <x>580</x>
        <y>220</y>
        <title>Task 1</title>
        <assignPolicy>auto</assignPolicy>
        <flex>
            <id>t4_flex</id>
            <item>
                <dataRef>
                    <id>file</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>text</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>text_from_child</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>multichoice</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>multichoice_setter</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>children_tasks</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
        </flex>
    </transition>
</process>