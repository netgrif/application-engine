<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
    <id>test</id>
    <version>1.0.0</version>
    <title>Test</title>
    <defaultRole>true</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">TST</property>
    </properties>
    <processEvents/>
    <caseEvents/>
    <data type="number">
        <id>field</id>
        <title>Field</title>
        <init>0</init>
    </data>
    <transition>
        <id>task</id>
        <x>0</x>
        <y>0</y>
        <title>Task</title>
        <event type="finish">
            <id>task1</id>
            <actions phase="post">
                <action id="action_0">
					<![CDATA[
                    def tasks = findTasks( { it.transitionId.eq("work_task"); } );
                    assignTasks(tasks);
                    finishTasks(tasks[0..5]);
                    cancelTasks(tasks[7..9]);
                    ]]>
				</action>
            </actions>
        </event>
    </transition>
    <transition>
        <id>work_task</id>
        <x>0</x>
        <y>0</y>
        <title>Work task</title>
    </transition>
</process>