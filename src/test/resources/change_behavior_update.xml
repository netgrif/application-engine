	<data type="boolean">
		<id>boolean_0</id>
		<title>Edit behavior on transition</title>
		<init>false</init>
		<event type="set">
			<id>boolean_0_set</id>
			<actions phase="post">
				<action id="action_0">
					<![CDATA[
					log.info("boolean_3.rawValue: ${boolean_0.rawValue}")
					make text_0, editable on t1 when { boolean_0.rawValue }
					make text_0, required on t1 when { boolean_0.rawValue }
					make text_0, initial on t1 when { !boolean_0.rawValue }
					]]>
				</action>
			</actions>
		</event>
	</data>
	<data type="boolean">
		<id>boolean_1</id>
		<title>Edit behavior on transitions</title>
		<init>false</init>
		<event type="set">
			<id>boolean_1_set</id>
			<actions phase="post">
				<action id="action_1">
					<![CDATA[
					log.info("boolean_3.rawValue: ${boolean_1.rawValue}")
					make text_0, editable on transitions when { boolean_1.rawValue }
					make text_0, required on transitions when { boolean_1.rawValue }
					make text_0, visible on transitions when { !boolean_1.rawValue }
					make text_0, optional on transitions when { !boolean_1.rawValue }
					]]>
				</action>
			</actions>
		</event>
	</data>
	<data type="boolean">
		<id>boolean_2</id>
