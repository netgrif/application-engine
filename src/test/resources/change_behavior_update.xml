		<init>false</init>
		<event type="set">
			<id>boolean_2_set</id>
			<actions phase="post">
				<action id="action_2">
					<![CDATA[
					log.info("boolean_3.rawValue: ${boolean_2.rawValue}")
					make text_0, editable on ([t1, t2]) when { boolean_2.rawValue }
					make text_0, visible on ([t1, t2]) when { !boolean_2.rawValue }
					]]>
				</action>
			</actions>
		</event>
