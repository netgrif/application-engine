                    <event type="set">
                        <id>t4set</id>
                        <actions phase="post">
                            <action id="action_0">
								<![CDATA[
                                I18nString optionD = new I18nString("David", ["sk": "Dávid"])
                                I18nString optionE = new I18nString("Eve", ["sk": "Eva"])
                                I18nString optionF = new I18nString("Felix", ["sk": "Félix"])
                                change enumeration_map_changed options {
                                    [
                                            "david": optionD,
                                            "eve": optionE,
                                            "felix": optionF
                                    ]
                                }
                                change enumMap value {
                                    "eve"
                                }
                                change multichoice_map_changed options {
                                    [
                                            "david": optionD,
                                            "eve": optionE,
                                            "felix": optionF
                                    ]
                                }
                                change multiMap value {
                                    ["eve", "felix"] as Set
                                }
                                ]]>
							</action>
                        </actions>
                    </event>
                </dataRef>
