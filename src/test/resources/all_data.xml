    <transition>
        <id>2</id>
        <x>500</x>
        <y>300</y>
        <cols>6</cols>
        <label>Test Action trigger</label>
        <assignPolicy>auto</assignPolicy>
        <dataRef>
            <id>testActionTrigger</id>
            <logic>
                <behavior>visible</behavior>
                <action trigger="set">
                    enumMap: f.enumeration_map_changed,
                    multiMap: f.multichoice_map_changed;

                    def optionD = new com.netgrif.application.engine.petrinet.domain.I18nString("David");
                    optionD.addTranslation("sk", "Dávid");
                    def optionE = new com.netgrif.application.engine.petrinet.domain.I18nString("Eve");
                    optionE.addTranslation("sk", "Eva");
                    def optionF = new com.netgrif.application.engine.petrinet.domain.I18nString("Felix");
                    optionF.addTranslation("sk", "Félix");

                    change enumMap options {
                    ["david": optionD,
                    "eve": optionE,
                    "felix": optionF]
                    }

                    change enumMap value {
                    "eve"
                    }

                    change multiMap options {
                    ["david": optionD,
                    "eve": optionE,
                    "felix": optionF]
                    }

                    change multiMap value {
                    ["eve", "felix"]
                    }
                </action>
            </logic>
        </dataRef>
