<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
	<id>agreement</id>
	<version>1.0.0</version>
	<title>Agreement</title>
	<icon>device_hub</icon>
	<defaultRole>true</defaultRole>
	<anonymousRole>true</anonymousRole>
	<properties>
		<property key="initials">Agr</property>
	</properties>
	<processEvents/>
	<caseEvents/>
	<data type="text">
		<id>contract_name</id>
		<title>Contract name</title>
		<placeholder>Input contract name</placeholder>
	</data>
	<data type="text">
		<id>customer_details</id>
		<title/>
		<init>Customer details</init>
	</data>
	<data type="text">
		<id>decision_result</id>
		<title>Agreement decision</title>
	</data>
	<data type="enumeration_map">
		<id>issue_type</id>
		<title>Select issue type</title>
		<options>
			<option key="formal">formal</option>
			<option key="fundamental">fundamental</option>
		</options>
	</data>
	<data type="taskRef">
		<id>taskRef_0</id>
		<title/>
		<init>t1</init>
	</data>
	<data type="taskRef">
		<id>taskRef_1</id>
		<title/>
		<init>t4</init>
	</data>
	<data type="taskRef">
		<id>taskRef_result</id>
		<title/>
		<init>t13</init>
	</data>
	<transition>
		<id>t1</id>
		<x>60</x>
		<y>340</y>
		<title>contract</title>
		<flex>
			<id>t1_flex</id>
			<item>
				<dataRef>
					<id>contract_name</id>
					<logic>
						<behavior>editable</behavior>
					</logic>
				</dataRef>
				<properties>
					<order>0</order>
					<flex-grow>0</flex-grow>
					<flex-shrink>1</flex-shrink>
				</properties>
			</item>
			<item>
				<dataRef>
					<id>customer_details</id>
					<logic>
						<behavior>visible</behavior>
					</logic>
				</dataRef>
				<properties>
					<order>0</order>
					<flex-grow>0</flex-grow>
					<flex-shrink>1</flex-shrink>
				</properties>
			</item>
		</flex>
	</transition>
	<transition>
		<id>t13</id>
		<x>60</x>
		<y>500</y>
		<title>result</title>
		<flex>
			<id>t13_flex</id>
			<item>
				<dataRef>
					<id>decision_result</id>
					<logic>
						<behavior>visible</behavior>
					</logic>
				</dataRef>
				<properties>
					<order>0</order>
					<flex-grow>0</flex-grow>
					<flex-shrink>1</flex-shrink>
				</properties>
			</item>
		</flex>
	</transition>
	<transition>
		<id>t4</id>
		<x>60</x>
		<y>420</y>
		<title>comment</title>
		<flex>
			<id>t4_flex</id>
			<item>
				<dataRef>
					<id>issue_type</id>
					<logic>
						<behavior>editable</behavior>
						<required>true</required>
					</logic>
				</dataRef>
				<properties>
					<order>0</order>
					<flex-grow>0</flex-grow>
					<flex-shrink>1</flex-shrink>
				</properties>
			</item>
		</flex>
	</transition>
	<transition>
		<id>t6</id>
		<x>140</x>
		<y>340</y>
		<title>summary A</title>
		<flex>
			<id>t6_flex</id>
			<item>
				<dataRef>
					<id>taskRef_1</id>
					<logic>
						<behavior>visible</behavior>
					</logic>
				</dataRef>
				<properties>
					<order>0</order>
					<flex-grow>0</flex-grow>
					<flex-shrink>1</flex-shrink>
				</properties>
			</item>
			<item>
				<dataRef>
					<id>taskRef_0</id>
					<logic>
						<behavior>visible</behavior>
					</logic>
				</dataRef>
				<properties>
					<order>0</order>
					<flex-grow>0</flex-grow>
					<flex-shrink>1</flex-shrink>
				</properties>
			</item>
			<item>
				<dataRef>
					<id>taskRef_result</id>
					<logic>
						<behavior>visible</behavior>
					</logic>
				</dataRef>
				<properties>
					<order>0</order>
					<flex-grow>0</flex-grow>
					<flex-shrink>1</flex-shrink>
				</properties>
			</item>
		</flex>
	</transition>
</process>