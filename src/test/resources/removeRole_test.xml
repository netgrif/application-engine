<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
    <id>test</id>
    <version>1.0.0</version>
    <title>Test</title>
    <defaultRole>false</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">TST</property>
    </properties>
    <caseName>Nová organizácia</caseName>
    <processEvents/>
    <caseEvents/>
    <role>
        <id>admin</id>
        <title>admin</title>
        <event type="assign">
            <id>admin_assign</id>
            <actions phase="pre">
                <action id="action_0">
					<![CDATA[
                    assignRole("manager", "test", findUserByEmail("test@mail.sk"))
                    ]]>
				</action>
            </actions>
        </event>
        <event type="remove">
            <id>admin_remove</id>
            <actions phase="pre">
                <action id="action_1">
					<![CDATA[
                    removeRole("manager", "test", findUserByEmail("test@mail.sk") )
                    ]]>
				</action>
            </actions>
        </event>
    </role>
    <role>
        <id>manager</id>
        <title>manager</title>
    </role>
</process>