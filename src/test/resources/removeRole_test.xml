<?xml version="1.0" encoding="UTF-8"?>
<document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="https://petriflow.com/petriflow.schema.xsd">
    <id>test</id>
    <initials>TST</initials>
    <title>Test</title>
    <defaultRole>false</defaultRole>

    <caseName>Nová organizácia</caseName>
    <!-- ROLES -->
    <role>
        <id>admin</id>
        <title>admin</title>
        <event type="assign">
            <id>1</id>
            <actions phase="pre">
                <action>
                    assignRole("manager", "test", findUserByEmail("test@mail.sk"))
                </action>
            </actions>
        </event>
        <event type="cancel">
            <id>1</id>
            <actions phase="pre">
                <action>
                    removeRole("manager", "test", findUserByEmail("test@mail.sk") )
                </action>
            </actions>
        </event>
    </role>
    <role>
        <id>manager</id>
        <title>manager</title>
    </role>
</document>