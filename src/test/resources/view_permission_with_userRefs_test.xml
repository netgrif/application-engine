<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
    <id>vpt_userRefs</id>
    <version>1.0.0</version>
    <title>ViewPermissionTest test</title>
    <defaultRole>true</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">VPT</property>
    </properties>
    <roleRef>
        <id>view_neg_role</id>
        <caseLogic>
            <view>false</view>
        </caseLogic>
    </roleRef>
    <roleRef>
        <id>view_pos_role</id>
        <caseLogic>
            <view>true</view>
        </caseLogic>
    </roleRef>
    <roleRef>
        <id>view_ul_neg</id>
        <caseLogic>
            <view>false</view>
        </caseLogic>
    </roleRef>
    <roleRef>
        <id>view_ul_pos</id>
        <caseLogic>
            <view>true</view>
        </caseLogic>
    </roleRef>
    <role>
        <id>view_neg_role</id>
        <title>view role</title>
    </role>
    <role>
        <id>view_pos_role</id>
        <title>view role</title>
    </role>
    <data type="file">
        <id>file</id>
        <title>File</title>
    </data>
    <data type="text">
        <id>text</id>
        <title>Text</title>
        <init>text</init>
    </data>
    <data type="userList">
        <id>view_ul_neg</id>
        <title/>
    </data>
    <data type="userList">
        <id>view_ul_pos</id>
        <title/>
    </data>
    <transition>
        <id>t1</id>
        <x>1</x>
        <y>1</y>
        <title>Transition</title>
        <roleRef>
            <id>view_neg_role</id>
            <logic>
                <view>false</view>
            </logic>
        </roleRef>
        <roleRef>
            <id>view_pos_role</id>
            <logic>
                <view>true</view>
            </logic>
        </roleRef>
        <roleRef>
            <id>view_ul_neg</id>
            <logic>
                <view>false</view>
            </logic>
        </roleRef>
        <roleRef>
            <id>view_ul_pos</id>
            <logic>
                <view>true</view>
            </logic>
        </roleRef>
        <flex>
            <id>t1_flex</id>
            <item>
                <dataRef>
                    <id>text</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
            <item>
                <dataRef>
                    <id>file</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
        </flex>
    </transition>
</process>