<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../main/resources/petriNets/petriflow_schema.xsd">
    <id>function_res</id>
    <version>1.0.0</version>
    <title>Test</title>
    <defaultRole>false</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">SFR</property>
    </properties>
    <function scope="namespace" name="calculate">
        <![CDATA[
		{ def x, def y ->
		    return x * y
		}
		]]>
    </function>
    <function scope="namespace" name="updateUseCaseFieldValue">
        <![CDATA[
		{ def fieldId, def newValue ->
		    change useCase.dataSet.get(fieldId) value { newValue }
		}
		]]>
    </function>
    <data type="number">
        <id>number</id>
        <title>Number</title>
    </data>
    <transition>
        <id>1</id>
        <x>0</x>
        <y>0</y>
        <title>Test</title>
        <flex>
            <id>1_flex</id>
            <item>
                <dataRef>
                    <id>number</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
        </flex>
    </transition>
</process>