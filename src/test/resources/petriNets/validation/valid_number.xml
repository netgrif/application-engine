            </validation>
        </validations>
    </data>
    <data type="number">
        <id>number03</id>
        <title/>
        <validations>
            <validation>
                <name>positive</name>
                <message>error-number03</message>
            </validation>
        </validations>
    </data>
    <data type="number">
        <id>number04</id>
