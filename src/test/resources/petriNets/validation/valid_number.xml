    </properties>
    <data type="number">
        <id>number01</id>
        <title/>
        <validations>
            <validation>
                <name>odd</name>
                <message>error-number01</message>
            </validation>
        </validations>
    </data>
    <data type="number">
        <id>number02</id>
        <title/>
        <validations>
            <validation>
