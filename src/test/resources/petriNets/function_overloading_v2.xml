<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../main/resources/petriNets/petriflow_schema.xsd">
    <id>function_overloading</id>
    <version>1.0.0</version>
    <title>Test</title>
    <defaultRole>false</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">FOF</property>
    </properties>
    <function scope="process" name="overloadingTest">
        <![CDATA[
		{ String x, String y ->
		    "$x $y"
		}
		]]>
    </function>
    <function scope="process" name="overloadingTest">
        <![CDATA[
		{ Double x, Double y ->
		    x * y
		}
		]]>
    </function>
    <data type="number">
        <id>number</id>
        <title>Number</title>
        <event type="set">
            <actions phase="post">
                <action id="action_1">
					<![CDATA[
                    change number2 value { overloadingTest(number.rawValue, number.rawValue) }
                    change text value { overloadingTest("${number.rawValue}" as String, "${number.rawValue}" as String) }
                    ]]>
				</action>
            </actions>
        </event>
    </data>
    <data type="number">
        <id>number2</id>
        <title>Number2</title>
    </data>
    <data type="text">
        <id>text</id>
        <title>Number</title>
    </data>
    <transition>
        <id>1</id>
        <x>0</x>
        <y>0</y>
        <title>Test</title>
        <flex>
            <id>1_flex</id>
        </flex>
    </transition>
</process>