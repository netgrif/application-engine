<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../main/resources/petriNets/petriflow_schema.xsd">
    <id>changed_fields_allowed_nets</id>
    <version>1.0.0</version>
    <title>New Model</title>
    <defaultRole>true</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">NEW</property>
    </properties>
    <processEvents/>
    <caseEvents/>
    <data type="caseRef">
        <id>caseRef</id>
        <title/>
    </data>
    <data type="caseRef">
        <id>caseRef2</id>
        <title/>
        <allowedNets>
            <allowedNet>org_group</allowedNet>
        </allowedNets>
    </data>
    <data type="text">
        <id>text</id>
        <title/>
        <event type="set">
            <id>text_set</id>
            <actions phase="post">
                <action id="action_0">
					<![CDATA[
                    change caseRef allowedNets {
                        [useCase.processIdentifier]
                    }
                    change caseRef2 allowedNets {
                        caseRef2.getAllowedNets() + useCase.processIdentifier
                    }
                    ]]>
				</action>
            </actions>
        </event>
    </data>
    <transition>
        <id>t1</id>
        <x>501</x>
        <y>247</y>
        <title>Task</title>
        <flex>
            <id>t1_flex</id>
            <item>
                <dataRef>
                    <id>text</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
        </flex>
    </transition>
</process>