<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../main/resources/petriNets/petriflow_schema.xsd">
    <id>dynamic_choices</id>
    <version>1.0.0</version>
    <title>dynamic_choices</title>
    <defaultRole>true</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">DVT</property>
    </properties>
    <caseName dynamic="true" id="title">"SK text value ${1+2+3}"</caseName>
    <data type="number">
        <id>number</id>
        <title>number</title>
        <init dynamic="true">
            <![CDATA[
			1 + 2 + 3
			]]>
        </init>
    </data>
    <data type="text">
        <id>text</id>
        <title>text</title>
        <init dynamic="true">
            <![CDATA[
			"text value ${number.rawValue as Integer}" as String
			]]>
        </init>
    </data>
    <i18n locale="en">
        <i18nString id="title">"EN text value ${1+2+3}"</i18nString>
    </i18n>
    <transition>
        <id>transition</id>
        <x>200</x>
        <y>200</y>
        <title>Transition</title>
        <assignPolicy>auto</assignPolicy>
        <flex>
            <id>transition_flex</id>
        </flex>
    </transition>
</process>