<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
    <id>constructor_destructor</id>
    <version>1.0.0</version>
    <title>Constructor and Destructor</title>
    <icon>business</icon>
    <defaultRole>false</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">CAD</property>
    </properties>
    <roleRef>
        <id>process_role</id>
        <caseLogic>
            <create>true</create>
            <delete>false</delete>
        </caseLogic>
    </roleRef>
    <processEvents>
        <event type="upload">
            <id>upload</id>
            <actions phase="post">
                <action id="action_0">
					<![CDATA[
                    createCase("constructor_destructor", "Construct")
                    ]]>
				</action>
            </actions>
        </event>
    </processEvents>
    <caseEvents>
        <event type="create">
            <id>create</id>
            <actions phase="post">
                <action id="action_1">
					<![CDATA[
                    change text value { "Its working..." }
                    ]]>
				</action>
            </actions>
        </event>
        <event type="delete">
            <id>pdf</id>
            <actions phase="post">
                <action id="action_2">
					<![CDATA[
                    createCase("constructor_destructor", "Destruct")
                    ]]>
				</action>
            </actions>
        </event>
    </caseEvents>
    <role>
        <id>process_role</id>
        <title>Process role</title>
    </role>
    <data type="text">
        <id>text</id>
        <title>Text</title>
        <init>text</init>
    </data>
</process>