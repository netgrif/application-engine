<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../main/resources/petriNets/petriflow_schema.xsd">
    <id>test</id>
    <version>1.0.0</version>
    <title>Test</title>
    <defaultRole>false</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">TST</property>
    </properties>
    <processEvents/>
    <caseEvents/>
    <role>
        <id>role1</id>
        <title>View</title>
    </role>
    <role>
        <id>role2</id>
        <title>Perform</title>
    </role>
    <data type="text">
        <id>data1</id>
        <title>Name</title>
    </data>
    <transition>
        <id>t1</id>
        <x>300</x>
        <y>100</y>
        <title>Perform</title>
        <roleRef>
            <id>role2</id>
            <permission>
                <perform>true</perform>
            </permission>
        </roleRef>
        <flex>
            <id>t1_flex</id>
            <item>
                <dataRef>
                    <id>data1</id>
                    <logic>
                        <behavior>editable</behavior>
                    </logic>
                </dataRef>
                <properties>
                    <order>0</order>
                    <flex-grow>0</flex-grow>
                    <flex-shrink>1</flex-shrink>
                </properties>
            </item>
        </flex>
    </transition>
    <transition>
        <id>t2</id>
        <x>300</x>
        <y>180</y>
        <title>View</title>
        <roleRef>
            <id>role1</id>
            <permission>
                <view>true</view>
            </permission>
        </roleRef>
    </transition>
</process>