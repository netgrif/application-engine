	<data type="file">
		<id>file_0</id>
		<title>Import translations</title>
		<event type="set">
			<id>file_0_set</id>
			<actions phase="pre">
				<action id="3">
					<!--@formatter:off-->
					file: f.file_0,
					text: f.translations;

					if (file.value?.path) {
						String json = org.apache.commons.io.FileUtils.readFileToString(new File(file.value?.path), java.nio.charset.StandardCharsets.UTF_8);
						change text value { json }
					}
					<!--@formatter:on-->
				</action>
			</actions>
		</event>
	</data>
	<data type="text" immediate="true">
		<id>translations</id>
		<title>Translations</title>
	</data>
	<data type="file">
		<id>file_svg</id>
		<title>Import SVG Icon</title>
		<event type="set">
			<id>file_svg_set</id>
			<actions phase="pre">
				<action id="4">
					<!--@formatter:off-->
					file: f.file_svg,
					text: f.svgIcon;

					if (file.value?.path) {
					String svg = org.apache.commons.io.FileUtils.readFileToString(new File(file.value?.path), java.nio.charset.StandardCharsets.UTF_8);
					change text value { svg }
					}
					<!--@formatter:on-->
				</action>
			</actions>
		</event>
	</data>
