	<transition>
		<id>8</id>
		<x>500</x>
		<y>340</y>
		<label>Add user</label>
		<assignPolicy>auto</assignPolicy>
		<grid>
			<id>8_grid</id>
			<item>
				<dataRef>
					<id>user_action_selection</id>
					<logic>
						<behavior>editable</behavior>
					</logic>
				</dataRef>
				<properties>
