    <caseName id="case_name">New filter</caseName>
    <!-- CASE EVENTS -->
    <caseEvents>
        <event type="create">
            <id>create</id>
            <actions phase="post">
                <action>
                    caseId: f.filter_case_id;

                    change caseId value {return useCase.stringId};
                </action>
            </actions>
        </event>
    </caseEvents>
    <!-- TRANSACTIONS -->
    <!-- ROLES -->
    <role>
        <id>system</id>
        <title>System</title>
    </role>
    <!-- PROCESS ROLE REFS -->
    <!-- PROCESS USER REFS -->
    <!-- FUNCTIONS -->
    <function scope="process" id="initializeMyFullFilterTaskRef">
        {
            com.netgrif.application.engine.petrinet.domain.dataset.TaskField myFullFilter
                ->
                change myFullFilter value {return [findTask({it.caseId.eq(useCase.stringId).and(it.transitionId.eq("view_filter"))}).stringId]}
        }
    </function>
