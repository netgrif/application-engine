<process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../petriflow_schema.xsd">
    <id>filter</id>
    <version>1.0.0</version>
    <title id="process_title">Filter</title>
    <icon>filter_alt</icon>
    <defaultRole>true</defaultRole>
    <anonymousRole>false</anonymousRole>
    <properties>
        <property key="initials">FTR</property>
    </properties>
    <caseName id="case_name">New filter</caseName>
    <processEvents/>
    <caseEvents>
        <event type="create">
            <id>case_create</id>
            <actions phase="post">
                <action id="action_0">
					<![CDATA[
                    change filter_case_id value {return useCase.stringId}
                    ]]>
				</action>
            </actions>
        </event>
    </caseEvents>
    <role>
        <id>system</id>
        <title>System</title>
    </role>
    <function scope="process" name="initializeMyFullFilterTaskRef">
        <![CDATA[
		{ com.netgrif.application.engine.petrinet.domain.dataset.TaskField myFullFilter ->
		    change myFullFilter value {return [findTask({it.caseId.eq(useCase.stringId).and(it.transitionId.eq("view_filter"))}).stringId]}
		}
		]]>
    </function>
    <data type="multichoice_map">
        <id>allowed_nets</id>
        <title id="allowed_nets">Allowed nets</title>
    </data>
    <data type="button">
        <id>and_me</id>
        <title/>
        <placeholder id="and">AND</placeholder>
    </data>
    <data type="button">
        <id>and_view</id>
        <title/>
        <placeholder id="and">AND</placeholder>
    </data>
    <data type="i18n">
        <id>divider_1</id>
        <title/>
        <component>
            <id>divider</id>
        </component>
    </data>
    <data type="filter" immediate="true">
        <id>filter</id>
        <title id="filter_field">Filter</title>
    </data>
    <data type="text">
        <id>filter_case_id</id>
        <title/>
    </data>
    <data type="enumeration_map" immediate="true">
        <id>filter_type</id>
        <title id="filter_type">Filter type</title>
        <options>
            <option key="Case" id="filter_type_case">Case filter</option>
            <option key="Task" id="filter_type_task">Task filter</option>
        </options>
    </data>
    <data type="i18n" immediate="true">
        <id>i18n_filter_name</id>
        <title/>
    </data>
    <data type="number">
        <id>is_imported</id>
        <title>imported</title>
        <init>0</init>
        <event type="set">
            <id>is_imported_set</id>
            <actions phase="post">
                <action id="action_3">
					<![CDATA[
                    change is_not_imported value { return 0 }
                    ]]>
				</action>
            </actions>
        </event>
    </data>
    <data type="number">
        <id>is_not_imported</id>
        <title>not imported</title>
        <init>1</init>
    </data>
    <data type="text">
        <id>menu_identifier</id>
        <title/>
    </data>
    <data type="text">
        <id>missing_allowed_nets</id>
        <title id="missing_allowed_nets">Missing processes</title>
        <desc id="missing_allowed_nets_desc">List of missing processes for current filter</desc>
        <component>
            <id>htmltextarea</id>
        </component>
    </data>
    <data type="enumeration_map">
        <id>missing_nets_translation</id>
        <title/>
        <options>
            <option key="sk">Zoznam chýbajúcich procesov pre aktuálny filter:</option>
            <option key="en">List of missing processes for current filter:</option>
            <option key="de">Liste der fehlenden Prozesse für den aktuellen Filter:</option>
        </options>
    </data>
    <data type="taskRef">
        <id>my_full_filter</id>
        <title/>
    </data>
    <data type="text">
        <id>new_title</id>
        <title id="filter_name">Filter name</title>
    </data>
    <data type="text" immediate="true">
        <id>origin_view_id</id>
        <title id="origin_view_id">Origin ViewId</title>
        <event type="set">
            <id>origin_view_id_set</id>
            <actions phase="post">
                <action id="action_1">
					<![CDATA[
                    if (origin_view_id.rawValue == "") {
                        return
                    }
                    // the same regex is used in the frontend FilterExtractionService. Please keep them in sync
                    def match = origin_view_id.rawValue =~ "^.*?(-\\d+)?\$"
                    match.matches()
                    def trimmed_id
                    if (match.start(1) == -1) {
                        trimmed_id = origin_view_id.rawValue
                    } else {
                        trimmed_id = origin_view_id.rawValue.substring(0, match.start(1))
                    }
                    change trimmed_origin_view_id value {trimmed_id}
                    make and_view, visible on view_filter when {true}
                    make and_view, visible on view_as_ancestor when {true}
                    make trimmed_origin_view_id, visible on t1 when {true}
                    make trimmed_origin_view_id, visible on t2 when {true}
                    make origin_view_id, hidden on t1 when {true}
                    make origin_view_id, hidden on t2 when {true}
                    ]]>
				</action>
            </actions>
        </event>
    </data>
    <data type="text" immediate="true">
        <id>parent_filter_id</id>
        <title id="parent_filter_id">parent filter ID</title>
        <event type="set">
            <id>parent_filter_id_set</id>
            <actions phase="post">
                <action id="action_2">
					<![CDATA[
                    def ancestor_view_filter = findTask({it.caseId.eq(parent_filter_id.rawValue).and(it.task.transitionId.eq("view_as_ancestor"))})
                    change taskref_and_parent value {[ancestor_view_filter.stringId]}
                    ]]>
				</action>
            </actions>
        </event>
    </data>
    <data type="taskRef">
        <id>taskref_and_parent</id>
        <title/>
    </data>
    <data type="text">
        <id>trimmed_origin_view_id</id>
        <title id="filter_from_view_id">Filter from view with ID</title>
    </data>
    <data type="enumeration_map" immediate="true">
        <id>visibility</id>
        <title id="filter_visibility">Filter visibility</title>
        <options>
            <option key="public" id="filter_visibility_public">Public</option>
            <option key="private" id="filter_visibility_private">Private</option>
        </options>
        <init>public</init>
    </data>
    <i18n locale="sk">
