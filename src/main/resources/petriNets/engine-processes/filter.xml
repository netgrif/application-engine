    <transition>
        <id>auto_create</id>
        <x>500</x>
        <y>180</y>
        <title>Automated new filter</title>
        <event type="finish">
            <id>auto_create_finish</id>
            <actions phase="post">
                <action id="action_6">
					<![CDATA[
                    initializeMyFullFilterTaskRef(my_full_filter)
                    ]]>
				</action>
            </actions>
        </event>
    </transition>
    <transition>
        <id>frontend_create</id>
        <x>380</x>
        <y>260</y>
        <title>Set filter metadata</title>
        <event type="finish">
            <id>frontend_create_finish</id>
            <actions phase="post">
                <action id="action_5">
					<![CDATA[
                    initializeMyFullFilterTaskRef(my_full_filter)
                    ]]>
				</action>
            </actions>
        </event>
    </transition>
    <transition>
        <id>import_filter</id>
        <x>740</x>
        <y>180</y>
        <title id="import_filter">Import filter</title>
        <event type="finish">
            <id>import_filter_finish</id>
            <actions phase="post">
                <action id="action_7">
					<![CDATA[
                    changeCaseProperty "title" about { new_title.rawValue }
                    ]]>
				</action>
            </actions>
        </event>
    </transition>
    <transition>
        <id>newFilter</id>
        <x>620</x>
        <y>260</y>
        <title id="create_new_filter">Create new filter</title>
        <assignPolicy>auto</assignPolicy>
        <event type="finish">
            <id>newFilter_finish</id>
            <title id="save">Save</title>
            <actions phase="post">
                <action id="action_4">
					<![CDATA[
                    useCase.setTitle(new_title.rawValue)
                    workflowService.save(useCase)
                    ]]>
				</action>
            </actions>
        </event>
    </transition>
    <transition>
        <id>t2</id>
        <x>740</x>
        <y>380</y>
        <title id="details">Details</title>
    </transition>
    <transition>
        <id>view_as_ancestor</id>
        <x>451</x>
        <y>418</y>
        <title/>
        <roleRef>
            <id>system</id>
            <permission>
