<document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="../petriflow_schema_layout.xsd">
    <id>all_data_configuration</id>
    <initials>PAD</initials>
    <title>All Data Configuration</title>
    <icon>toc</icon>
    <defaultRole>true</defaultRole>
    <anonymousRole>true</anonymousRole>
    <transitionRole>false</transitionRole>
    <transition>
        <id>petriflow_all_data</id>
        <x>0</x>
        <y>0</y>
        <label>All Data</label>
        <icon>toc</icon>
        <grid>
            <id>petriflow_all_data_flex</id>
            <properties>
                <display>grid</display>
                <grid-template-columns>repeat(2, 1fr)</grid-template-columns>
                <justify-items>stretch</justify-items>
                <align-items>stretch</align-items>
                <justify-content>stretch</justify-content>
                <align-content>stretch</align-content>
            </properties>
        </grid>
    </transition>
</document>